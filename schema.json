{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"data": {
			"type": "object",
			"properties": {
				"quotes": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"author": {
								"type": "string"
							},
							"content": {
								"type": "string"
							}
						},
						"required": ["author", "content"]
					}
				}
			},
			"required": ["quotes"]
		}
	},
	"required": ["data"]
}
